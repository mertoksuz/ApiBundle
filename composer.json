{
  "name": "nedra/rest-bundle",
  "description": "Generates a REST api based on your entities",
  "type": "symfony-bundle",
  "keywords" : [
    "symfony based api bundle"
  ],
  "license" : [
    "MIT"
  ],
  "minimum-stability": "beta",
  "require": {
    "php": ">=5.6",
    "symfony/dependency-injection": "^3.3",
    "symfony/framework-bundle": "^3.3",
    "symfony/http-kernel": "^3.3",
    "symfony/routing": "^3.3",
    "symfony/twig-bundle": "^3.3",
    "symfony/yaml": "^3.3",
    "symfony-cmf/routing-bundle": "^2.0",
    "doctrine/doctrine-bundle": "^1.6",
    "friendsofsymfony/rest-bundle": "^2.1",
    "jms/serializer-bundle": "^1.1",
    "gedmo/doctrine-extensions": "^2.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "symfony/symfony": "^2.8|^3.3"
  },
  "authors": [
    {
      "name": "Mert Öksüz",
      "email": "mert@linux.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nedra\\RestBundle\\": "",
      "Nedra\\RestBundle\\Tests\\": "Tests/"
    }
  }
}
